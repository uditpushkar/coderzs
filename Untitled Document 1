    <div id="ftab-1-12913" class="tab2inner padleft40 ui-tabs-panel ui-widget-content ui-corner-bottom">
        <h3 class="printTitle">Encyclopedic Entry</h3>
        
        

    

		
			






    
        
            <div class="colgroup col610 {{options.subclass}}"><div class="wrap-right"><div class="wrap-left">
        




    
    
        
            
                
                <div class="col1 full_width">
                    
                    

                    
                    



    
    




<div class="reverse_rail">
    <div class="right">
        
        

    

		
			






    
        
            <div class="colgroup col610 {{options.subclass}}"><div class="wrap-right"><div class="wrap-left">
        




    
    
        
            
                
